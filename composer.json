{
    "name": "bbaga/buildkite-php-guzzle-client",
    "description": "PSR-18 Wrapper for Guzzle to be used with bbaga/buildkite-php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bela Bagameri",
            "email": "bagameri.bela@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "bbaga\\BuildkiteApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bbaga\\BuildkiteApi\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    }
}
